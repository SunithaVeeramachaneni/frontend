{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "59cd7bdb-c04d-46c1-b17a-57a4d58730ef"
  },
  "version": 1,
  "defaultProject": "cwp",
  "newProjectRoot": "projects",
  "projects": {
    "cwp": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./custom-webpack.config.js"
            },
            "outputPath": "dist/cwp",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/plyr/dist/plyr.css",
              "src/scss/style.scss",
              "node_modules/ngx-spinner/animations/ball-clip-rotate-pulse.css"
            ],
            "scripts": [
              "node_modules/popper.js/dist/umd/popper.min.js",
              "src/assets/external_api.js"
            ],
            "aot": false,
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "allowedCommonJsDependencies": [
              "quill-delta-to-html",
              "buffer",
              "uuid",
              "isomorphic-unfetch",
              "fast-xml-parser",
              "@aws-crypto/crc32",
              "@aws-crypto/sha256-browser",
              "@aws-crypto/sha256-js",
              "camelcase-keys",
              "lodash/get",
              "lodash/isEmpty",
              "lodash/isEqual",
              "lodash/flatten",
              "ulid"
            ],
            "serviceWorker": true,
            "ngswConfigPath": "ngsw-config.json"
          },
          "configurations": {
            "development": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                }
              ],
              "aot": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "buildOptimizer": false,
              "sourceMap": true,
              "optimization": false,
              "namedChunks": true
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.production.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              // "sourceMap": false,
              "sourceMap": {
                "scripts": true,
                "styles": true,
                "hidden": true,
                "vendor": true
              },
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "1MB"
                }
              ]
            },
            "ci": {
              "progress": false
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "cwp:build"
          },
          "configurations": {
            "development": {
              "browserTarget": "cwp:build:development"
            },
            "production": {
              "browserTarget": "cwp:build:production"
            },
            "ci": {
              "progress": false
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-builders/custom-webpack:extract-i18n",
          "options": {
            "browserTarget": "cwp:build"
          }
        },
        "test": {
          "builder": "@angular-builders/custom-webpack:karma",
          "options": {
            "customWebpackConfig": {
              "path": "./custom-webpack.config.js"
            },
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "styles": [
              "src/scss/style.scss",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/ngx-spinner/animations/ball-clip-rotate-pulse.css"
            ],
            "scripts": [
              "node_modules/popper.js/dist/umd/popper.min.js",
              "src/assets/external_api.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest"
            ]
          },
          "configurations": {
            "ci": {
              "progress": false,
              "watch": false
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "cwp:serve"
          },
          "configurations": {
            "devlopment": {
              "devServerTarget": "cwp:serve:devlopment"
            },
            "production": {
              "devServerTarget": "cwp:serve:production"
            },
            "ci": {
              "devServerTarget": "cwp:serve:ci"
            }
          }
        }
      }
    }
  }
}
